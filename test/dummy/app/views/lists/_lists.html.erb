<table>
<% @lists.each do |list| %>
  <tr>
    <td><%= list.name %></td>
    <td><%= link_to 'Delete', list, :data => { :confirm => 'Are you sure?' } , :method => :delete, :remote => true %></td>
    <td><%= link_to 'Edit', edit_list_path(list), :method => :get, :remote => true %></td>
    <td><%= link_to 'Look', list_entries_path(list) %></td>
  </tr>
<% end %>
</table>
